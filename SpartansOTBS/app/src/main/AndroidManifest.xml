<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="otbs.spartans.com.spartansotbs">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".home"
            android:label="@string/page1">
            <intent-filter>
                <action android:name="otbs.spartans.com.spartansotbs.home" />
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            </activity>
        <activity android:name=".book"
            android:label="@string/page2">
            <intent-filter>
                <action android:name="otbs.spartans.com.spartansotbs.book" />
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".tInfo"
            android:label="@string/page3">
            <intent-filter>
                <action android:name="otbs.spartans.com.spartansotbs.tInfo" />
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".contact"
            android:label="@string/page4">
            <intent-filter>
                <action android:name="otbs.spartans.com.spartansotbs.contact" />
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

    </application>

</manifest>